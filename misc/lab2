if &cp | set nocp | endif
let s:cpo_save=&cpo
set cpo&vim
map! <S-Insert> <MiddleMouse>
nmap K \he
vmap [% [%m'gv``
nmap \rvil :exe "unlet! g:VIlisp_loaded | so /home/home/.vim/plugin/VIlisp.vim"
nmap \hI :call VIlisp_hyperspec( "index-page", 0 )
nmap \hi :call VIlisp_hyperspec( "index", 0 )
nmap \hg :call VIlisp_hyperspec( "grep", 1 )
nmap \hs :call VIlisp_hyperspec( "suffix", 1 )
nmap \hp :call VIlisp_hyperspec( "prefix", 1 )
nmap \he :call VIlisp_hyperspec( "exact", 0 )
nmap \cc m`[(iv%a:'<,'>s/^/; /``%/(
nmap \il [(%a)h%i(
nmap \fs 99\fc
nmap \fc [(=%`'
nmap \ms 99[(V%
nmap \lb \tr
nmap \tr :call VIlisp_goto_buffer_or_window( VIlisp_last_lisp )
nmap \sb :exe "hide bu" g:VIlisp_scratch
nmap \wtb :sb | call VIlisp_goto_buffer_or_window( g:VIlisp_test )
nmap \tb :call VIlisp_goto_buffer_or_window( g:VIlisp_test )
nmap \clf \cf\laf
nmap \cf :call VIlisp_send_to_lisp( "(compile-file \"" . expand( "%:p" ) . "\")\n")
nmap \lcf \laf
nmap \laf :call VIlisp_send_to_lisp( "(load \"" . expand( "%:p:r" ) . "\")\n")
nmap \lf :call VIlisp_send_to_lisp( "(load \"" . expand( "%:p" ) . "\")\n")
nmap \ct :call VIlisp_copy_sexp_to_test()
nmap \ci :call VIlisp_send_to_lisp( "" )
nmap \qi :call VIlisp_send_to_lisp( "(quit)\n" )
nmap \ri :call VIlisp_send_to_lisp( "q\n" )
vmap \ec \eb
vmap \es \eb
vmap \eb :call VIlisp_eval_block()
nmap \ec :call VIlisp_eval_next_sexp_lisp()
nmap \es :call VIlisp_eval_defun_lisp()
vmap ]% ]%m'gv``
vmap a% [%v]%
nmap gx <Plug>NetrwBrowseX
nnoremap <silent> <Plug>NetrwBrowseX :call netrw#NetBrowseX(expand("<cWORD>"),0)
map <S-Insert> <MiddleMouse>
